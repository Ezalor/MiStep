plugins {
    id 'com.android.application'
}

android {
    compileSdk 33

    defaultConfig {
        applicationId "pf.mistep"
        minSdk 21
        targetSdk 33
        versionCode 1
        versionName "1.0"

        // 如果有需要，可以添加 testInstrumentationRunner 等配置
    }

    buildTypes {
        release {
            minifyEnabled false
            proguardFiles getDefaultProguardFile('proguard-android-optimize.txt'), 'proguard-rules.pro'
        }
        debug {
            // debug 特有配置
        }
    }
}

dependencies {
    implementation 'androidx.appcompat:appcompat:1.6.1'
    implementation 'com.google.android.material:material:1.9.0'
    // 根据项目需要添加更多依赖
}
